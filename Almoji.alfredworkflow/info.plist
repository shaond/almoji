				<string>Searching for emojis...</string>
				<key>script</key>
				<string>#!/bin/bash

# Get the query from Alfred
query="$1"

# Get configuration variables (with defaults)
skin_tone="${skin_tone:-}"
gender="${gender:-}"

# Build the almoji command
cmd="almoji"

# Add skin tone if set
if [ -n "$skin_tone" ]; then
    cmd="$cmd --skin-tone $skin_tone"
fi

# Add gender if set
if [ -n "$gender" ]; then
    cmd="$cmd --gender $gender"
fi

# Add query
cmd="$cmd --limit 20 $query"

# Execute almoji and capture output
emoji_results=$(eval $cmd 2&gt;/dev/null)

# Check if we got results
if [ -z "$emoji_results" ]; then
    echo '{"items": [{"title": "No emojis found", "subtitle": "Try a different search term", "valid": false}]}'
    exit 0
fi

# Start building JSON output
echo -n '{"items": ['

first=true
while IFS= read -r line; do
    # Parse the emoji and keyword from output like "ðŸ”¥ (fire)"
    if [[ $line =~ ^(.*)[[:space:]]\(([^)]+)\)$ ]]; then
        emoji="${BASH_REMATCH[1]}"
        keyword="${BASH_REMATCH[2]}"

        # Add comma separator for all but first item
        if [ "$first" = true ]; then
            first=false
        else
            echo -n ','
        fi

        # Build JSON item
        cat &lt;&lt;EOF
{
    "uid": "$keyword",
    "title": "$emoji",
    "subtitle": "$keyword",
    "arg": "$emoji",
    "text": {
        "copy": "$emoji",
        "largetype": "$emoji"
    },
    "valid": true
}
EOF
    fi
done &lt;&lt;&lt; "$emoji_results"

echo -n ']}'</string>
